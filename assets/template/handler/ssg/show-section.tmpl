{{ define "page" }}
{{ template "layout" . }}
{{ end }}

{{ define "title" }}
{{ .Data.Name }}
{{ end }}

{{ define "content" }}
<div class="space-y-4">
    <h1 class="text-2xl font-bold">{{ .Data.Name }}</h1>
    <p class="text-gray-600">{{ .Data.Description }}</p>
    <p class="text-sm text-gray-500">Path: {{ .Data.Path }}</p>
</div>
{{ end }}

{{ define "submenu" }}
{{ template "menu" . }}
{{ end }}
